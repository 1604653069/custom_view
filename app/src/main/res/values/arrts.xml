<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RatingBar">
        <attr name="normal" format="reference"/>
        <attr name="selected" format="reference"/>
        <attr name="starNum" format="integer"/>
        <attr name="defaultSelectedNum" format="integer"/>
        <attr name="star_padding" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="ClockView">
        <attr name="circle_color" format="color" />
        <attr name="hour_color" format="color" />
        <attr name="minute_color" format="color" />
        <attr name="second_color" format="color" />
        <attr name="quarter_mark_color" format="color" />
        <attr name="minute_mark_color" format="color" />
        <attr name="draw_center_circle" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TimeView">
        <attr name="borderWidth" format="dimension"/>
        <attr name="borderColor" format="color"/>
        <attr name="circleBackground" format="color"/>
        <attr name="minScaleColor" format="color"/>
        <attr name="midScaleColor" format="color"/>
        <attr name="maxScaleColor" format="color"/>
        <attr name="minScaleLength" format="dimension"/>
        <attr name="midScaleLength" format="dimension"/>
        <attr name="maxScaleLength" format="dimension"/>
        <attr name="textColor" format="color"/>
        <attr name="textSize" format="dimension"/>
        <attr name="isDrawText" format="boolean"/>
        <attr name="secondPointerColor" format="color"/>
        <attr name="minPointerColor" format="color"/>
        <attr name="hourPointerColor" format="color"/>
        <attr name="secondPointerLength" format="dimension"/>
        <attr name="minPointerLength" format="dimension"/>
        <attr name="hourPointerLength" format="dimension"/>
        <attr name="secondPointerSize" format="dimension"/>
        <attr name="minPointerSize" format="dimension"/>
        <attr name="hourPointerSize" format="dimension"/>
        <attr name="centerPointColor" format="color"/>
        <attr name="centerPointSize" format="dimension"/>
        <attr name="centerPointRadiu" format="dimension"/>
        <attr name="centerPointType" format="string"/>
        <attr name="isSecondGoSmooth" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="RadarView">
        <attr name="num" format="integer|reference"/>
        <attr name="radarGridColor" format="color|reference"/>
        <attr name="radarGridStrokeWidth" format="dimension|reference"/>
        <attr name="radarLineColor" format="color|reference"/>
        <attr name="radarLineStrokeWidth" format="dimension|reference"/>
        <attr name="circleColor" format="color|reference"/>
        <attr name="circleWidth" format="dimension|reference"/>
        <attr name="layerColor" format="color|reference"/>
        <attr name="android:textSize" format="color|reference"/>
        <attr name="android:textColor" format="dimension|reference"/>
    </declare-styleable>
</resources>